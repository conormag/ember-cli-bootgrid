	{{#if headers}}
		<thead>
			<tr>
			{{#each headers as |header|}}
				<th 
					data-column-id="{{header.id}}" 
					data-type="{{header.type}}"
					data-identifier="{{header.identifier}}">
					{{header.name}}
				</th>
			{{/each}}
			</tr>
		</thead>
{{/if}}
{{#if rows}}
		<tbody>
			{{#each rows as |row|}}
				{{#each-keys object=row as |key value|}}
					<td>{{value}}</td>
				{{/each-keys}}
			{{/each}}
		</tbody>
{{/if}}
{{yield}}
